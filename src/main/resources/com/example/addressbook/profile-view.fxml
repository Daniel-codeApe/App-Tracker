<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.addressbook.ProfileController">
   <children>
      <Circle fill="WHITE" layoutX="304.0" layoutY="87.0" radius="66.0" stroke="BLACK" strokeType="INSIDE" />
      <Text layoutX="287.0" layoutY="92.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Profile" />
      <Label layoutX="262.0" layoutY="174.0" text="Username:">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>
      <Label fx:id="username" alignment="CENTER" contentDisplay="CENTER" layoutX="265.0" layoutY="209.0" prefHeight="17.0" prefWidth="80.0" textAlignment="CENTER" wrapText="true" />
      <Label layoutX="281.0" layoutY="226.0" text="Email:">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>
      <Label fx:id="email" alignment="CENTER" contentDisplay="CENTER" layoutX="226.0" layoutY="261.0" prefHeight="17.0" prefWidth="159.0" textAlignment="CENTER" wrapText="true" />
      <Button fx:id="updateButton" layoutX="278.0" layoutY="293.0" mnemonicParsing="false" onAction="#setUpdateButton" text="Update" />
      <Button fx:id="toDashboard" layoutX="465.0" layoutY="21.0" mnemonicParsing="false" onAction="#backToDashboard" text="Back to Dashboard" />
   </children>
</AnchorPane>
